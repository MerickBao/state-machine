<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.TransLogDAO">
	<insert id="addLog" parameterType="com.example.demo.domain.TransLogEntity">
		insert into t_trans_log (machineId, transId) values (#{machineId}, #{transId})
	</insert>
	<select id="getTransLogByMachineId" parameterType="java.lang.Integer" resultType="java.util.List">
		select * from t_trans_log where machineId = #{id}
	</select>
</mapper>
